// select all fields for full access in Permission Sets / Profiles
// (open Developer Console, select Console and Paste the following code)

 (function() {
    var aa = document.querySelectorAll("input[type=checkbox]");
    for (var i = 0; i < aa.length; i++){
	if(i<12 || i%2==0){
        	aa[i].checked = true;
	}
    }
 })()
